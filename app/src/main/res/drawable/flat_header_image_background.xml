<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
  <item>
    <shape>
      <stroke android:width="1dp"
          android:color="@color/color_text_white" />
      <padding android:left="@dimen/flat_image_border_white_padding"
          android:top="@dimen/flat_image_border_white_padding"
          android:right="@dimen/flat_image_border_white_padding"
          android:bottom="@dimen/flat_image_border_white_padding" />
      <corners android:radius="20dp" />
    </shape>
  </item>
  <item>
    <shape>
      <stroke android:width="3dp"
          android:color="@color/FlatItemWindowsBar" />
      <padding android:left="@dimen/flat_image_border_green_padding"
          android:top="@dimen/flat_image_border_green_padding"
          android:right="@dimen/flat_image_border_green_padding"
          android:bottom="@dimen/flat_image_border_green_padding" />
      <corners android:radius="20dp" />
    </shape>
  </item>
</layer-list>